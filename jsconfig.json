{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "#/*": ["src/*"],
      "styles/*": ["styles/*"],
      "reducer/*": ["reducer/*"],
      "constants/*": ["constants/*"],
      "selectors/*": ["selectors/*"],
      "components/*": ["components/*"]
    },
    "include": ["src"],
    "exclude": ["node_modules", "build"]
  }
}
